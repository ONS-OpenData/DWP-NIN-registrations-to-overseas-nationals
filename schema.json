{
  "@context": ["http://www.w3.org/ns/csvw", {"@language": "en"}],
  "tables": [
    {
      "url": "out/nationalities.csv",
      "tableSchema": "https://ons-opendata.github.io/ref_common/codelist-schema.json"
    }, {
      "url": "out/ons_geo_observations.csv",
      "tableSchema": {
        "columns": [{
          "titles": "Period",
          "name": "period",
          "datatype": {
            "base": "string",
            "format": "year/[0-9]{4}|quarter/[0-9]{4}-Q[1-4]"
          }
        }, {
          "titles": "Nationality",
          "name": "nationality",
          "required": true
        }, {
          "titles": "Measure Type",
          "name": "measureType",
          "datatype": {
            "format": "Count"
          }
        }, {
          "titles": "Value",
          "name": "value",
          "datatype": "string",
          "required": true
        }, {
          "titles": "Unit",
          "name": "unit",
          "datatype": {
            "base": "string",
            "format": "People"
          },
          "required": true
        }, {
          "titles": "Geography",
          "name": "geography",
          "datatype": "string",
          "required": true
        }],
        "foreignKeys": [{
          "columnReference": "nationality",
          "reference": {
            "resource": "out/nationalities.csv",
            "columnReference": "notation"
          }
        }],
        "primaryKey": ["period", "nationality", "measureType", "unit", "geography"]
      }
    }
  ]
}
